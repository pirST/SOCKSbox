{
  "manifest_version": 3,
  "name": "SOCKSbox",
  "version": "0.2",
  "description": "Extension to set multiple SOCKS5 proxy",
  "permissions": ["proxy", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
